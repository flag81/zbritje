{
  "cli": {
    "version": ">= 10.2.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store",
        "autoIncrement": "version",
        "image": "latest",
        "credentialsSource": "remote"
      },
      "env": {
        "EXPO_NO_DEV": "true",
        "EXPO_MINIFY": "true",
        "GOOGLE_SERVICE_INFO": "@flag81/GOOGLE_SERVICE_INFO"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
