{
  "cli": {
    "version": ">= 10.2.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_DEV": "true",
        "EXPO_MINIFY": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
